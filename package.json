{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"prisma/**/*.ts\" \"*.{mjs,ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"prisma/**/*.ts\" \"*.{mjs,ts,json}\"",
    "db:seed": "tsx prisma/seed.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "turbopack-prisma": "rm -rf .next node_modules/.prisma node_modules/.cache && pnpm prisma generate"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
