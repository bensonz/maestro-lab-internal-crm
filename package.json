{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && PRISMA_MIGRATE_ADVISORY_LOCK_TIMEOUT=60000 DATABASE_URL=${DATABASE_URL_UNPOOLED:-$DATABASE_URL} prisma migrate deploy && next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"prisma/**/*.ts\" \"*.{mjs,ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"prisma/**/*.ts\" \"*.{mjs,ts,json}\"",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:status": "prisma migrate status",
    "test": "vitest",
    "test:run": "vitest run",
    "turbopack-prisma": "rm -rf .next node_modules/.prisma node_modules/.cache && pnpm prisma generate"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
